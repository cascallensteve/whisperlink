{
  "version": 2,
  "builds": [
    {
      "src": "whisperlink_backend/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "feedback/static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/feedback/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "whisperlink_backend/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "whisperlink_backend.settings"
  },
  "functions": {
    "whisperlink_backend/wsgi.py": {
      "maxDuration": 30
    }
  }
}
